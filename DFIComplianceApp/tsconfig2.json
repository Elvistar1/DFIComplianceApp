{
  "rules": {
    "companies": {
      "$companyId": {
        ".write": "auth != null",
        ".validate": "newData.child('FileNumber').val().matches(/^[0-9]{2,}$/)"
      }
    },
    "byFileNumber": {
      "$fileNo": {
        ".write": "!data.exists()"
      }
    },
    "byCertificateNumber": {
      "$certNo": {
        ".write": "!data.exists()"
      }
    }
  }
}
